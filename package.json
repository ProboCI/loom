{
  "name": "probo-loom",
  "version": "2.0.1",
  "description": "A stream storage service that allows you to store a stream with some metadata, subscribe to streams, and load the contents of those streams later.",
  "main": "index.js",
  "author": "",
  "license": "Proprietary",
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.9.4",
    "co-mocha": "^1.1.2",
    "eslint": "^1.10.3",
    "eslint-config-probo": "^1.0.2",
    "mocha": "^5.1.1",
    "supertest": "^1.1.0",
    "temp": "^0.8.3",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "test": "./node_modules/.bin/mocha -t 12000 --require ts-node/register --recursive test/*.ts --exit",
    "watch": "./node_modules/.bin/mocha -w -t 12000 --require ts-node/register --recursive test/*.ts",
    "_test": "./node_modules/.bin/mocha -w --no-timeouts --require ts-node/register --recursive test/*.ts",
    "start": "node ./dist/bin/loom.js -c /etc/probo/loom.yaml",
    "start2": "nodemon --require ./node_modules/ts-node/register/type-check ./bin/loom.ts -c /etc/probo/loom.yaml",
    "startdev": "nodemon --inspect=0.0.0.0:9229 --require ts-node/register/type-check ./bin/loom.ts -c /etc/probo/loom.yaml | ./node_modules/.bin/bunyan #-o short",
    "spy": "curl --no-buffer localhost:3060/spy?color",
    "tsc": "$(npm bin)/tsc",
    "data-migration": "nodemon --require ts-node/register/type-check ./dataMigration/0002-rethink-to-postgre.ts -c /etc/probo/loom.yaml | ./node_modules/.bin/bunyan",
    "build": "npm run clean && tsc"
  },
  "dependencies": {
    "@types/bl": "^0.8.31",
    "@types/chai": "^4.1.3",
    "@types/knex": "^0.14.12",
    "@types/lodash": "^4.14.104",
    "@types/through2": "^2.0.33",
    "@types/yargs": "^11.0.0",
    "bl": "^1.2.1",
    "bun": "0.0.11",
    "bunyan": "^1.4.0",
    "chai": "^4.1.2",
    "co": "^4.6.0",
    "colors": "^1.1.2",
    "g": "^2.0.1",
    "gulp-uglify": "^1.5.4",
    "knex": "^0.13.0",
    "knex-migrate": "^1.3.0",
    "lodash": "^3.10.1",
    "memorystream": "^0.3.1",
    "ms": "^0.7.1",
    "nodemon": "^1.17.3",
    "passport": "^0.2.2",
    "passport-http-bearer": "^1.0.1",
    "pg": "^6.2.2",
    "restify": "^4.0.0",
    "tailing-stream": "^0.2.0",
    "thinky": "^2.2.4",
    "through2": "^2.0.0",
    "ts-node": "^5.0.0",
    "uuid": "^3.2.1",
    "yaml-config-loader": "^2.0.1",
    "yargs": "^3.31.0"
  }
}
