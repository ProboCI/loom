{
  "name": "probo-loom",
  "version": "1.0.2",
  "description": "A stream storage service that allows you to store a stream with some metadata, subscribe to streams, and load the contents of those streams later.",
  "main": "index.js",
  "author": "",
  "license": "Proprietary",
  "devDependencies": {
    "bl": "^1.0.0",
    "co-mocha": "^1.1.2",
    "mocha": "^2.2.5",
    "should": "^7.0.4"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha -t 8000 --require should --recursive",
    "watch": "./node_modules/.bin/mocha -w -t 8000 --require should --recursive",
    "_test": "./node_modules/.bin/mocha -w --no-timeouts --require should --recursive",
    "start": ". $PWD/env.sh && node index.js",
    "startdev": ". $PWD/env.sh && nodemon index.js | ./node_modules/.bin/bunyan #-o short",
    "spy": "curl --no-buffer localhost:3060/spy?color"
  },
  "dependencies": {
    "JSONStream": "^1.0.4",
    "bunyan": "^1.4.0",
    "co": "^4.6.0",
    "colors": "^1.1.2",
    "lodash": "^3.10.1",
    "memorystream": "^0.3.1",
    "multistream": "^2.0.2",
    "passport": "^0.2.2",
    "passport-http-bearer": "^1.0.1",
    "restify": "^4.0.0",
    "rethinkdbdash": "^2.1.6",
    "stream-combiner": "^0.2.2",
    "stream-throttle": "^0.1.3",
    "through2": "^2.0.0"
  }
}
